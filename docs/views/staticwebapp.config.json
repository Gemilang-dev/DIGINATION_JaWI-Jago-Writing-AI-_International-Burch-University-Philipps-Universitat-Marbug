{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/js/*", "/css/*", "/data/*"]
  },
  "routes": [
    {
      "route": "/data/*",
      "headers": {
        "Content-Type": "application/json"
      }
    }
  ],
  "mimeTypes": {
    ".json": "application/json"
  }
}